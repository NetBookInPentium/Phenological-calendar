    <div class="d7">
        <form action="" method="get">
            <input name="s" placeholder="Поиск культур" type="search">
            <button type="submit">Поиск</button>
        </form>
    </div>


<p role="status">Current count: @currentCount</p>

<button class="btn btn-primary" @onclick="IncrementCount">Рассчитать</button>

@code {
    Phenologicalcalendar.Call_DB Call_db = new Phenologicalcalendar.Call_DB();
    private string currentCount = "иди нахуй";


    private void IncrementCount()
    {
        currentCount = Call_db.Test("SELECT * FROM testdb.testes");

        List<string> strings = Call_db.Test_list("SELECT * FROM testdb.testes");
        for(int i = 0; i < strings.Count; i++)
        {
            Console.WriteLine(i + " элемент = " + strings[i]);
        }

    }
}
